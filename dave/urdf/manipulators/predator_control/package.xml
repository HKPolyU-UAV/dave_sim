<?xml version="1.0"?>
<package format="2">
  <name>predator_control</name>
  <version>0.1.1</version>
  <description>Package with configuration and launch files to control the predator manipulator.</description>
  <author email="khakyya@gmail.com">Youssef Khaky</author>
  <maintainer email="khakyya@gmail.com">Youssef Khaky</maintainer>

  <license>Apache-2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>uuv_manipulators_kinematics</exec_depend>
  <exec_depend>uuv_manipulators_control</exec_depend>
  <exec_depend>controller_manager</exec_depend>
  <exec_depend>uuv_teleop</exec_depend>
  <exec_depend>ros_controllers</exec_depend>
  <exec_depend>ros_control</exec_depend>
  <exec_depend>gazebo_ros_control</exec_depend>
  <exec_depend>effort_controllers</exec_depend>

